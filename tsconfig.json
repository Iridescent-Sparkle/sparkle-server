{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/email": [
        "libs/email/src"
      ],
      "@app/email/*": [
        "libs/email/src/*"
      ],
      "@app/redis": [
        "libs/redis/src"
      ],
      "@app/redis/*": [
        "libs/redis/src/*"
      ],
      "@app/db": [
        "libs/db/src"
      ],
      "@app/db/*": [
        "libs/db/src/*"
      ],
      "@app/config": [
        "libs/config/src"
      ],
      "@app/config/*": [
        "libs/config/src/*"
      ],
      "@app/etcd": [
        "libs/etcd/src"
      ],
      "@app/etcd/*": [
        "libs/etcd/src/*"
      ],
      "@app/jwt": [
        "libs/jwt/src"
      ],
      "@app/jwt/*": [
        "libs/jwt/src/*"
      ],
      "@app/winston": [
        "libs/winston/src"
      ],
      "@app/winston/*": [
        "libs/winston/src/*"
      ],
      "@app/rabbitmq": [
        "libs/rabbitmq/src"
      ],
      "@app/rabbitmq/*": [
        "libs/rabbitmq/src/*"
      ],
      "@app/sms": [
        "libs/sms/src"
      ],
      "@app/sms/*": [
        "libs/sms/src/*"
      ],
      "@app/im": [
        "libs/im/src"
      ],
      "@app/im/*": [
        "libs/im/src/*"
      ]
    }
  }
}